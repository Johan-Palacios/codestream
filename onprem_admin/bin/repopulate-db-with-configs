#!/bin/bash

echo "Press enter to go..."
read

set -x

../../shared/codestream_configs/bin/mongo-config-loader.js --mongo-url mongodb://localhost/codestream --add-cfg-file $CSSVC_CFG_FILE --desc "Original version for schema"
for r in `seq 1 8`; do
	../../shared/codestream_configs/bin/mongo-config-loader.js --mongo-url mongodb://localhost/codestream --add-cfg-file $CSSVC_CFG_FILE --desc "revision number $r"
done
../../shared/codestream_configs/bin/mongo-config-loader.js --mongo-url mongodb://localhost/codestream --report-cfg --schema-version 14
